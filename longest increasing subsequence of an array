#include <bits/stdc++.h>
using namespace std;

int longestIncreasingSubseq(vector<int> &arr,int n){
    vector<int> dp(n,1);  //for i=0 i.e len 1 arr will have lcs=1 always
    int maxx=1;
    for(int i=1;i<n;i++){
        for(int j=0;j<i;j++){
            if(arr[i]>arr[j]){
                dp[i]=max(dp[i],dp[j]+1); 
                maxx=max(maxx,dp[i]);
            }
        }
    }
    return maxx;
}

int main() {
    int n;
    cin>>n;
    vector<int> arr(n);
    for(int i=0;i<n;i++) cin>>arr[i];
    cout<<longestIncreasingSubseq(arr,n)<<endl;
	return 0;
}

input:
8
10 9 2 5 3 7 101 18
Output:
4
